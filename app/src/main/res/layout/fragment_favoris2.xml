    <View
        android:id="@+id/imageView_start_favs3"
        android:layout_width="112dp"
        android:layout_height="111dp"
        android:background="#CCCCCC"
        app:layout_constraintBottom_toBottomOf="@+id/rectangleView2"
        app:layout_constraintEnd_toEndOf="@+id/rectangleView"
        app:layout_constraintHorizontal_bias="0.155"
        app:layout_constraintStart_toStartOf="@+id/rectangleView2"
        app:layout_constraintTop_toTopOf="@+id/rectangleView2"
        app:layout_constraintVertical_bias="0.459"
        tools:src="@drawable/rectangle_research_1st_view" />

    <TextView
        android:id="@+id/secondcard"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Appart Vincennes"
        android:textStyle="bold"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/rectangleView2"
        app:layout_constraintEnd_toEndOf="@+id/rectangleView2"
        app:layout_constraintHorizontal_bias="0.183"
        app:layout_constraintStart_toEndOf="@+id/imageView_start_favs3"
        app:layout_constraintTop_toTopOf="@+id/rectangleView2"
        app:layout_constraintVertical_bias="0.248" />

    <TextView
        android:id="@+id/secondcard2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="5.0"
        android:textSize="13dp"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/rectangleView2"
        app:layout_constraintEnd_toEndOf="@+id/rectangleView2"
        app:layout_constraintHorizontal_bias="0.076"
        app:layout_constraintStart_toEndOf="@+id/imageView_start_favs3"
        app:layout_constraintTop_toBottomOf="@+id/secondcard"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="16dp"
        android:layout_height="17dp"
        android:layout_marginEnd="68dp"
        android:src="@drawable/baseline_star_24"
        app:layout_constraintBottom_toBottomOf="@+id/rectangleView2"
        app:layout_constraintEnd_toEndOf="@+id/rectangleView2"
        app:layout_constraintHorizontal_bias="0.116"
        app:layout_constraintStart_toEndOf="@+id/secondcard2"
        app:layout_constraintTop_toBottomOf="@+id/secondcard"
        app:layout_constraintVertical_bias="0.115" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:src="@drawable/baseline_location_on_24"
        app:layout_constraintBottom_toBottomOf="@+id/rectangleView2"
        app:layout_constraintEnd_toEndOf="@+id/rectangleView2"
        app:layout_constraintHorizontal_bias="0.075"
        app:layout_constraintStart_toEndOf="@+id/imageView_start_favs3"
        app:layout_constraintTop_toBottomOf="@+id/secondcard2"
        app:layout_constraintVertical_bias="0.115" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
